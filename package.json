{
  "name": "rgm-webapp",
  "description": "rgm kpis",
  "version": "0.0.0",
  "themeColor": "#ffffff",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "npx google-artifactregistry-auth",
    "setup": "node scripts/setup.mjs",
    "start": "node -r tsconfig-paths/register build/bootstrap/main.js",
    "prebuild": "shx rm -rf ./build ./public",
    "build": "run-s build:*",
    "lint": "eslint ./ --fix",
    "build:i18n": "node scripts/i18n.mjs",
    "build:app": "remix build",
    "build:bootstrap": "tsc --project bootstrap/tsconfig.json",
    "build:sw": "esbuild --outfile=public/sw.js --bundle --minify ./app/entry.worker.ts",
    "build:icons": "node ./scripts/icons.mjs",
    "dev": "cross-env NODE_ENV=development remix build && run-p dev:*",
    "dev:i18n": "nodemon --watch 'app/routes/**/_i18n/*.json' --ext json scripts/i18n.mjs",
    "dev:app": "cross-env NODE_ENV=development remix watch --debug --sourcemap",
    "dev:bootstrap": "nodemon bootstrap/main.ts",
    "dev:redis": "docker-compose up",
    "test": "run-p test:*",
    "test:unit": "cross-env NODE_ENV=test vitest",
    "validate": "run-p validate:*",
    "validate:lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ./app ./bootstrap ./tests",
    "validate:type": "tsc -b",
    "hooks:clean": "rm -rf .husky || true",
    "hooks:install-all": "npm run hooks:install && npm run hooks:install-btdp ",
    "hooks:install": "npm run hooks:clean && husky install && mkdir .husky/btdp-hooks",
    "hooks:install-btdp": "./scripts/shell/import-btdp-hooks.sh",
    "hooks:add-standard-hooks": "npm run hooks:pre-push && npm run hooks:pre-commit",
    "hooks:pre-push": "npm run hooks:add \"pre-push\" \"npm run test\"",
    "hooks:pre-commit": "npm run hooks:add \"pre-commit\" \"npm run lint\"",
    "hooks:add": "sh ./scripts/shell/generate-hook.sh"
  },
  "dependencies": {
    "@c4e-lib/ui": "^0.0.3",
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@google-cloud/bigquery": "^6.1.0",
    "@google-cloud/logging-winston": "^5.3.0",
    "@remix-run/express": "^1.13.0",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@remix-validated-form/with-zod": "^2.0.5",
    "accept-language-parser": "^1.5.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "framer-motion": "^9.0.3",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-chained-backend": "^4.2.0",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-backend": "^2.1.1",
    "i18next-localstorage-backend": "^4.1.0",
    "isbot": "^3.6.6",
    "ms": "^2.1.3",
    "numeral": "^2.0.6",
    "nunjucks": "^3.2.3",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-i18next": "^11.18.6",
    "react-icons": "^4.8.0",
    "redis": "^4.6.4",
    "remix-auth-socials": "^2.0.4",
    "remix-i18next": "^5.0.0",
    "remix-utils": "^6.0.0",
    "remix-validated-form": "^4.6.8",
    "ts-extras": "^0.11.0",
    "tsconfig-paths": "^4.1.2",
    "usehooks-ts": "^2.9.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "zod": "^3.20.6",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/accept-language-parser": "^1.5.3",
    "@types/express": "^4.17.17",
    "@types/numeral": "^2.0.2",
    "@types/nunjucks": "^3.2.2",
    "@types/potrace": "^2.1.1",
    "@types/prompts": "^2.4.2",
    "@types/ramda": "^0.28.23",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.5",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.8",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^8.1.0",
    "glob-promise": "^6.0.2",
    "happy-dom": "^8.4.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "ora": "^6.1.2",
    "postcss": "^8.4.21",
    "potrace": "^2.1.8",
    "prettier": "^2.8.4",
    "prompts": "^2.4.2",
    "sharp": "^0.31.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "type-fest": "^3.5.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.5",
    "yargs": "^17.6.2"
  }
}
