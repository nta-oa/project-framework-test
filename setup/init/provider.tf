terraform {
  required_version = "~> 1.3"
  backend "gcs" {}

  required_providers {
    google = {
      source = "google"
      version = "<=4.65.2"
    }
  }
}

provider "google" {
  project = local.project
}

data "google_project" "default" {
  provider = google
}
